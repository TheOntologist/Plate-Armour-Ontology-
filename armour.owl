    </owl:Class>
    


    <!-- http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#Fauld -->

    <owl:Class rdf:about="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#Fauld">
        <rdfs:subClassOf rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#PlateArmour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#isProtectorOf"/>
                <owl:someValuesFrom rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#Waist"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">and a Faulds are a piece of plate armour worn below a breastplate to protect the waist and hips. They take the form of bands of metal surrounding both legs, potentially surrounding the entire hips in a form similar to a skirt.</rdfs:comment>
    </owl:Class>
    


