    <!-- http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#Arm -->

    <owl:Class rdf:about="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#Arm">
        <rdfs:subClassOf rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#BodyPart"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#UpperArm"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#Shoulder"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#Forearm"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#Elbow"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#Wrist"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#hasPart"/>
                <owl:someValuesFrom rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#Hand"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#ArmArmour -->

    <owl:Class rdf:about="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#ArmArmour">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#PlateArmour"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#isProtectorOf"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#Arm"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#isPartOf"/>
                                        <owl:someValuesFrom rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#Arm"/>
                                    </owl:Restriction>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#BackSelector -->

    <owl:Class rdf:about="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#BackSelector">
        <rdfs:subClassOf rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#FrontBackSelector"/>
        <owl:disjointWith rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#FrontSelector"/>
    </owl:Class>
    


    <!-- http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#BodyArmour -->

