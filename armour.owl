    <!-- http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#Vambrace -->

    <owl:Class rdf:about="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#Vambrace">
        <rdfs:subClassOf rdf:resource="http://www.cs.man.ac.uk/~stevensr/ontology/armour.owl#PlateArmour"/>
        <rdfs:subClassOf>
            <owl:Restriction>
